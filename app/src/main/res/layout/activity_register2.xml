<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".login.RegisterActivity2">

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/spacing_m"
        android:layout_marginTop="64dp"
        android:fontFamily="@font/notosanskr_bold"
        android:includeFontPadding="false"
        android:text="아래 내용에\n동의해주세요."
        android:textSize="24sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="@dimen/spacing_m"
        android:layout_marginTop="@dimen/spacing_xs"
        android:fontFamily="@font/notosanskr_regular"
        android:includeFontPadding="false"
        android:textSize="12sp"
        android:textColor="@color/sub_gray"
        android:text="원활한 서비스 제공을 위해\n이용약간 동의 등의 내용에 동의해주세요"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView" />

    <Button
        android:id="@+id/next_btn"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_margin="@dimen/spacing_m"
        android:background="@drawable/ic_disabled_button"
        android:enabled="false"
        android:fontFamily="@font/notosanskr_bold"
        android:text="다음"
        android:textColor="@color/sub_gray"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent" />

    <LinearLayout
        android:id="@+id/linearLayout2"
        android:layout_width="0dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="@dimen/spacing_l"
        android:orientation="vertical"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/textView2">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/check_all_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/spacing_l"
                android:layout_marginTop="@dimen/spacing_l"
                android:layout_marginRight="@dimen/spacing_xs"
                android:layout_marginBottom="@dimen/spacing_l"
                android:layout_weight="0"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/ic_disabled_check" />

            <TextView
                android:id="@+id/textView4"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/spacing_l"
                android:layout_marginBottom="@dimen/spacing_l"
                android:layout_weight="0"
                android:fontFamily="@font/notosanskr_regular"
                android:includeFontPadding="false"
                android:text="모두 동의합니다."
                android:textColor="@color/sub_black"
                android:textSize="16sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/item_border"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/check1_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/spacing_l"
                android:layout_marginTop="@dimen/spacing_m"
                android:layout_marginRight="@dimen/spacing_xs"
                android:layout_marginBottom="@dimen/spacing_m"
                android:layout_weight="0"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/ic_disabled_check2" />

            <TextView
                android:id="@+id/textView5"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/spacing_m"
                android:layout_marginBottom="@dimen/spacing_m"
                android:layout_weight="1"
                android:fontFamily="@font/notosanskr_regular"
                android:includeFontPadding="false"
                android:text="이용약관 (필수)"
                android:textColor="@color/sub_black"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/view1_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/spacing_m"
                android:layout_marginRight="@dimen/spacing_l"
                android:layout_marginBottom="@dimen/spacing_m"
                android:layout_weight="0"
                android:fontFamily="@font/notosanskr_regular"
                android:includeFontPadding="false"
                android:text="보기"
                android:textColor="@color/sub_gray"
                android:textSize="12sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/item_border"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/check2_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/spacing_l"
                android:layout_marginTop="@dimen/spacing_m"
                android:layout_marginRight="@dimen/spacing_xs"
                android:layout_marginBottom="@dimen/spacing_m"
                android:layout_weight="0"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/ic_disabled_check2" />

            <TextView
                android:id="@+id/textView7"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/spacing_m"
                android:layout_marginBottom="@dimen/spacing_m"
                android:layout_weight="1"
                android:fontFamily="@font/notosanskr_regular"
                android:includeFontPadding="false"
                android:text="개인정보처리방침 (필수)"
                android:textColor="@color/sub_black"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/view2_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/spacing_m"
                android:layout_marginRight="@dimen/spacing_l"
                android:layout_marginBottom="@dimen/spacing_m"
                android:layout_weight="0"
                android:fontFamily="@font/notosanskr_regular"
                android:includeFontPadding="false"
                android:text="보기"
                android:textColor="@color/sub_gray"
                android:textSize="12sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/item_border"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/check3_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/spacing_l"
                android:layout_marginTop="@dimen/spacing_m"
                android:layout_marginRight="@dimen/spacing_xs"
                android:layout_marginBottom="@dimen/spacing_m"
                android:layout_weight="0"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/ic_disabled_check2" />

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/spacing_m"
                android:layout_marginBottom="@dimen/spacing_m"
                android:layout_weight="1"
                android:fontFamily="@font/notosanskr_regular"
                android:includeFontPadding="false"
                android:text="커뮤니티 이용규칙 확인 (필수)"
                android:textColor="@color/sub_black"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/view3_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/spacing_m"
                android:layout_marginRight="@dimen/spacing_l"
                android:layout_marginBottom="@dimen/spacing_m"
                android:layout_weight="0"
                android:fontFamily="@font/notosanskr_regular"
                android:includeFontPadding="false"
                android:text="보기"
                android:textColor="@color/sub_gray"
                android:textSize="12sp" />
        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/item_border"
            android:orientation="horizontal">

            <ImageButton
                android:id="@+id/check4_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="@dimen/spacing_l"
                android:layout_marginTop="@dimen/spacing_m"
                android:layout_marginRight="@dimen/spacing_xs"
                android:layout_marginBottom="@dimen/spacing_m"
                android:layout_weight="0"
                android:background="@android:color/transparent"
                app:srcCompat="@drawable/ic_disabled_check2" />

            <TextView
                android:id="@+id/textView11"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/spacing_m"
                android:layout_marginBottom="@dimen/spacing_m"
                android:layout_weight="1"
                android:fontFamily="@font/notosanskr_regular"
                android:includeFontPadding="false"
                android:text="광고성 정보 수신 동의 (필수)"
                android:textColor="@color/sub_black"
                android:textSize="16sp" />

            <TextView
                android:id="@+id/view4_btn"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/spacing_m"
                android:layout_marginRight="@dimen/spacing_l"
                android:layout_marginBottom="@dimen/spacing_m"
                android:layout_weight="0"
                android:fontFamily="@font/notosanskr_regular"
                android:includeFontPadding="false"
                android:text="보기"
                android:textColor="@color/sub_gray"
                android:textSize="12sp" />

        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>